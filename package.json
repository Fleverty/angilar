	"scripts": {
		"build.aot": "node ./node_modules/webpack/bin/webpack.js --config webpack/app-aot.config.js",
		"build.app": "node ./node_modules/webpack/bin/webpack.js --config webpack/app.config.js",
		"build.polyfills": "node ./node_modules/webpack/bin/webpack.js --config webpack/polyfills.config.js",
		"build.vendors": "node ./node_modules/webpack/bin/webpack.js --config webpack/vendors.config.js",
		"build.test": "node ./node_modules/webpack/bin/webpack.js --config webpack/app-test.config.js",
		"watch.app.none": "npm run build.app -- --display none --watch",
		"karma": "node ./node_modules/karma/bin/karma start ./karma.conf.js",
		"watch": "npm run build.polyfills && npm run build.vendors && npm run build.app -- --watch",
		"watch.parallel": "npm-run-all --parallel karma watch.app.none"
	},
